{
  "scripts": {
    "start": "gunicorn park.wsgi:application -c gunicorn_config.py"
  },
  "version": 2,
  "builds": [
    {
      "src": "park",
      "use": "@now/python",
      "config": { "maxLambdaSize": "25mb" }
    },
    {
      "src": "park",
      "use": "@now/static-builder",
      "config": { "zeroConfig": true }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "gunicorn_config.py" }
  ]
}
